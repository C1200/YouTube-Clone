---
layout: none
---
{{ assign c = site.channel | where: 'uid', page.channel }}
{{ assign channel = c[0] }}
{
    "id": {{ page.vid | jsonify }},
    "title": {{ page.title | jsonify }},
    "description": {{ page.description | jsonify }},
    "channel": {
        "id": {{ page.channel | jsonify }},
        "name": {{ channel.title | jsonify }},
        "avatar": {{ channel.avatar | jsonify }},
        "subs": {{ channel.subs | jsonify }},
        "html_url": {{ channel.id | absolute_url | jsonify }}
    },
    "thumbnail": {{ page.thumbnail | jsonify }},
    "publish_date": {{ page.publish_date | jsonify }},
    "views": {{ page.views | jsonify }},
    "file": {{ page.file | jsonify }}
}